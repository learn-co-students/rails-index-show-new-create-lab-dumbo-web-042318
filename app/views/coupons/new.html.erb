<h1>Coupon Form</h1>
<%= form_for @coupon do |f| %>
  <%= f.label :coupon_code, "Coupon Code" %>
  <%= f.text_field  :coupon_code %>
  <%= f.label :store, "Store" %>
  <%= f.text_field  :store %>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= f.submit "Submit Coupon" %>
<% end %>
